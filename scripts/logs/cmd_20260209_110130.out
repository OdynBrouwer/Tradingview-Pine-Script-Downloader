[CAPTURE 0] '//@version=6\nstrategy("Bank Nifty RSI Dynamic v6", \n     overlay=true, \n     initial_capital=500000, \n     pyramiding=0, \n     margin_long=0, \n     margin_short=0)\n\n// --- Inputs ---\nint rsiLength      = input.int(14, "RSI Length")\nint rsiUpperEntry  = input.int(68, "RSI Put Entry (>68)")\nint rsiLowerEntry  = input.int(30, "RSI Call Entry (<30)")\nint rsiPutExit     = input.int(56, "RSI Put Exit (<56)")\nint rsiCallExit    = input.int(45, "RSI Call Exit (>45)")\n\n// --- Math for 3 Lots (90 Units) ---\nint totalUnits     = 90 \n\n// --- Indicators ---\nfloat rsiValue = ta.rsi(close, rsiLength)\n\n// --- Strategy Logic ---\n// 1. ENTRY CONDITIONS (On Candle Close)\nbool buyCall = ta.crossunder(rsiValue, rsiLowerEntry) \nbool buyPut  = ta.crossover(rsiValue, rsiUpperEntry)\n\n// 2. EXIT CONDITIONS (On Candle Close)\n// If we hold a Call, exit when RSI rises above 45\nbool exitCall = strategy.position_size > 0 and rsiValue > rsiCallExit\n// If we hold a Put, exit when RSI drops below 56\nbool exitPut  = strategy.position_size < 0 and rsiValue < rsiPutExit\n\n// --- Execution ---\n\n// Enter Trades\nif buyCall\n    strategy.entry("Call", strategy.long, qty=totalUnits, comment="RSI < 30")\n\nif buyPut\n    strategy.entry("Put", strategy.short, qty=totalUnits, comment="RSI > 68")\n\n// Exit Trades based on your RSI levels\nif exitCall\n    strategy.close("Call", comment="RSI > 45 Exit")\n\nif exitPut\n    strategy.close("Put", comment="RSI < 56 Exit")\n\n// --- Visuals ---\nplot(rsiValue, "RSI", color=color.purple, display=display.none)\nhline(rsiUpperEntry, "Put Entry", color=color.red)\nhline(rsiPutExit, "Put Exit", color=color.orange, linestyle=hline.style_dotted)\nhline(rsiCallExit, "Call Exit", color=color.blue, linestyle=hline.style_dotted)\nhline(rsiLowerEntry, "Call Entry", color=color.green)\n\n// Mark entries/exits on chart\nplotshape(buyCall, "Call Buy", shape.triangleup, location.belowbar, color.green, size=size.small)\nplotshape(buyPut, "Put Buy", shape.triangledown, location.abovebar, color.red, size=size.small)'
