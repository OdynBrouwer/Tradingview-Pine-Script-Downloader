             s_sum += lr\r\n                s_sq += lr*lr\r\n            float s_vol = math.sqrt(math.max(0.0, (s_sq/20.0)-(s_sum/20.0)*(s_sum/20.0))) * 1587.45\r\n            float vs = _gov_calc(s_vol, cfg.target_vol_safe, 0.5)\r\n            float r_hh = -1.0\r\n            for i=0 to 125\r\n                float v = _get_val(h_r_sf4, i)\r\n                if v > r_hh\r\n                    r_hh := v\r\n            float dd_arg = r_hh > 0 ? (r_now / r_hh - 1.0) : 0.0\r\n            float dd = _score_ddg(dd_arg, cfg.range_dd)\r\n            float cd = _arr_corr(h_r_sf4_chg, h_r_stk_chg, 20)\r\n            float cd_norm = (1.0 - cd) / 2.0\r\n            float sqs = (val_cp*cfg.w_cp + vs*cfg.w_vs + dd*cfg.w_ddg + cd_norm*cfg.w_cd) / sqs_div\r\n            float pen = 1.0\r\n            if (math.abs(r5) > cfg.shock_price/100.0) or (stk_vol > cfg.shock_vol)\r\n                if wroc < 0 and rc20 < -cfg.fall_thr/100.0 \r\n                    pen := cfg.penalty_val\r\n            if rc20 < -cfg.rate_fall/100.0 and math.abs(r5) > cfg.rate_accel/100.0\r\n                pen := cfg.penalty_val\r\n            sc4.sqs := sqs * pen\r\n            sc4.vol_gov := vs\r\n            if sc4.sqs >= cfg.baseline\r\n                sc4.passed := true\r\n                sc4.drop_code := DROP_OK\r\n            else\r\n                sc4.drop_code := DROP_BASELINE\r\n\r\n        // Safe 5\r\n        if str.length(s5) > 0 and array.size(h_sf5_c) > 0\r\n            valid_cnt := valid_cnt + 1\r\n            float r_now = _get_val(h_r_sf5, 0)\r\n            float r_l20 = _get_val(h_r_sf5, cfg.len_20)\r\n            float r_l5  = _get_val(h_r_sf5, 5)\r\n            float rc20  = _sdiv(r_now, r_l20) - 1.0\r\n            float r5    = _sdiv(r_now, r_l5) - 1.0\r\n            float val_cp = _score_cp(rc20, cfg.range_cp)\r\n            float s_sum=0.0, float s_sq=0.0\r\n            for i=0 to 19\r\n                float p0 = _get_val(h_sf5_c, i)\r\n                float p1 = _get_val(h_sf5_c, i+1)\r\n                float lr = (p0>0 and p1>0) ? math.log(p0/p1) : 0.0\r\n                s_sum += lr\r\n                s_sq += lr*lr\r\n            float s_vol = math.sqrt(math.max(0.0, (s_sq/20.0)-(s_sum/20.0)*(s_sum/20.0))) * 1587.45\r\n            float vs = _gov_calc(s_vol, cfg.target_vol_safe, 0.5)\r\n            float r_hh = -1.0\r\n            for i=0 to 125\r\n                float v = _get_val(h_r_sf5, i)\r\n                if v > r_hh\r\n                    r_hh := v\r\n            float dd_arg = r_hh > 0 ? (r_now / r_hh - 1.0) : 0.0\r\n            float dd = _score_ddg(dd_arg, cfg.range_dd)\r\n            float cd = _arr_corr(h_r_sf5_chg, h_r_stk_chg, 20)\r\n            float cd_norm = (1.0 - cd) / 2.0\r\n            float sqs = (val_cp*cfg.w_cp + vs*cfg.w_vs + dd*cfg.w_ddg + cd_norm*cfg.w_cd) / sqs_div\r\n            float pen = 1.0\r\n            if (math.abs(r5) > cfg.shock_price/100.0) or (stk_vol > cfg.shock_vol)\r\n                if wroc < 0 and rc20 < -cfg.fall_thr/100.0 \r\n                    pen := cfg.penalty_val\r\n            if rc20 < -cfg.rate_fall/100.0 and math.abs(r5) > cfg.rate_accel/100.0\r\n                pen := cfg.penalty_val\r\n            sc5.sqs := sqs * pen\r\n            sc5.vol_gov := vs\r\n            if sc5.sqs >= cfg.baseline\r\n                sc5.passed := true\r\n                sc5.drop_code := DROP_OK\r\n            else\r\n                sc5.drop_code := DROP_BASELINE\r\n\r\n        // Sort & Allocate\r\n        SafeCalcState[] arr = array.new<SafeCalcState>()\r\n        array.push(arr, sc1)\r\n        array.push(arr, sc2)\r\n        array.push(arr, sc3)\r\n        array.push(arr, sc4)\r\n        array.push(arr, sc5)\r\n        \r\n        int n_arr = array.size(arr)\r\n        for i = 0 to n_arr - 2\r\n            for j = 0 to n_arr - i - 2\r\n                SafeCalcState a = array.get(arr, j)\r\n                SafeCalcState b = array.get(arr, j + 1)\r\n                if a.sqs < b.sqs\r\n                    array.set(arr, j, b)\r\n                    array.set(arr, j + 1, a)\r\n                    \r\n        float sum_sqs = 0.0\r\n        int k_cnt = 0\r\n        \r\n        for i = 0 to n_arr - 1\r\n            SafeCalcState s = array.get(arr, i)\r\n            if s.passed\r\n                valid_pass := valid_pass + 1\r\n                if k_cnt < cfg.top_k\r\n                    sum_sqs := sum_sqs + s.sqs\r\n                    k_cnt := k_cnt + 1\r\n                else\r\n                    s.passed := false\r\n                    s.drop_code := DROP_NOT_TOPK\r\n                    array.set(arr, i, s)\r\n                    \r\n        k_cnt := 0\r\n        \r\n        used_safe_alloc := 0.0 \r\n        \r\n        if sum_sqs > 0\r\n            for i = 0 to n_arr - 1\r\n                SafeCalcState s = array.get(arr, i)\r\n                if s.passed and k_cnt < cfg.top_k\r\n                    float share = s.sqs / sum_sqs\r\n                    float alloc = safe_budget * share * s.vol_gov\r\n                    s.weight_share := alloc\r\n                    used_safe_alloc := used_safe_alloc + alloc\r\n                    array.set(arr, i, s)\r\n                    k_cnt := k_cnt + 1\r\n        \r\n        float saved_stk = math.max(0.0, raw_stk - final_stock)\r\n        float saved_safe = math.max(0.0, safe_budget - used_safe_alloc)\r\n        final_cash := saved_stk + saved_safe\r\n        \r\n        // Safety Valve\r\n        float check_sum = final_stock + used_safe_alloc + final_cash\r\n        if math.abs(check_sum - 1.0) > EPSILON\r\n            if check_sum > 1.0 + EPSILON\r\n                float norm = 1.0 / check_sum\r\n                final_stock := final_stock * norm\r\n                final_cash  := final_cash * norm\r\n                used_safe_alloc := used_safe_alloc * norm\r\n                for i = 0 to n_arr - 1\r\n                    SafeCalcState s = array.get(arr, i)\r\n                    s.weight_share := s.weight_share * norm\r\n                    array.set(arr, i, s)\r\n            else if check_sum < 1.0 - EPSILON\r\n                final_cash := final_cash + (1.0 - check_sum)\r\n            \r\n            float final_check = final_stock + used_safe_alloc + final_cash\r\n            if math.abs(final_check - 1.0) > EPSILON\r\n                rc := SUM_FAIL\r\n                is_ready := false\r\n                risk_gate := false\r\n                final_stock := 0.0\r\n                final_cash := 1.0\r\n                used_safe_alloc := 0.0\r\n                for i = 0 to n_arr - 1\r\n                    SafeCalcState s = array.get(arr, i)\r\n                    s.weight_share := 0.0\r\n                    array.set(arr, i, s)\r\n        \r\n        // Map Output to Hoisted Vars\r\n        for i = 0 to n_arr - 1\r\n            SafeCalcState s = array.get(arr, i)\r\n            if s.id == 1 \r\n                w1 := s.weight_share\r\n                sd1 := SafeDiag.new(str.length(s1)>0, (str.length(s1)>0 and not na(sf1_c)), s.sqs, w1, s.drop_code)\r\n            if s.id == 2 \r\n                w2 := s.weight_share\r\n                sd2 := SafeDiag.new(str.length(s2)>0, (str.length(s2)>0 and not na(sf2_c)), s.sqs, w2, s.drop_code)\r\n            if s.id == 3 \r\n                w3 := s.weight_share\r\n                sd3 := SafeDiag.new(str.length(s3)>0, (str.length(s3)>0 and not na(sf3_c)), s.sqs, w3, s.drop_code)\r\n            if s.id == 4 \r\n                w4 := s.weight_share\r\n                sd4 := SafeDiag.new(str.length(s4)>0, (str.length(s4)>0 and not na(sf4_c)), s.sqs, w4, s.drop_code)\r\n            if s.id == 5 \r\n                w5 := s.weight_share\r\n                sd5 := SafeDiag.new(str.length(s5)>0, (str.length(s5)>0 and not na(sf5_c)), s.sqs, w5, s.drop_code)\r\n        \r\n        // ID Extraction\r\n        if array.size(arr) > 0\r\n            t1_id := array.get(arr, 0).id\r\n        if array.size(arr) > 1\r\n            t2_id := array.get(arr, 1).id\r\n            \r\n    w := TrinityWeights.new(final_stock, w1, w2, w3, w4, w5, final_cash)\r\n    df := TrinityDiagFull.new(raw_stk, mvol, mdec, gov_final, safe_budget, t1_id, t2_id, final_stock+used_safe_alloc+final_cash)\r\n    \r\n    sum_check := math.abs((w.stock+w.safe1+w.safe2+w.safe3+w.safe4+w.safe5+w.cash) - 1.0) < EPSILON\r\n    \r\n    TrinityDiagLite dl = TrinityDiagLite.new(is_ready, rc, CORE_BUILD, risk_gate, wroc, valid_cnt, valid_pass, sum_check)\r\n    \r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5]\r\n\r\nexport calc_weights(string sym, string cp, string cs, string s1, string s2, string s3, string s4, string s5, TrinityConfig cfg) =>\r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5] = _core_compute(sym, cp, cs, s1, s2, s3, s4, s5, cfg)\r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5]\r\n\r\nexport calc_diagnostics(string sym, string cp, string cs, string s1, string s2, string s3, string s4, string s5, TrinityConfig cfg) =>\r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5] = _core_compute(sym, cp, cs, s1, s2, s3, s4, s5, cfg)\r\n    [df, sd1, sd2, sd3, sd4, sd5]\r\n\r\nexport calc_weights_flat(string sym, string cp, string cs, string s1, string s2, string s3, string s4, string s5, \r\n                         bool en,\r\n                         int l20, float w20, int l63, float w63, int l126, float w126, int l252, float w252,\r\n                         float rf, float fac, float tvs, float tvsf, float gmm, \r\n                         int kl, float kd, float ks,\r\n                         int tk, float bl, float wcp, float wcd, float wvs, float wdd,\r\n                         float sp, float sv, float ft, float rf2, float ra, \r\n                         float r_cp, float r_dd, float pen_v, \r\n                         int rb) =>\r\n    TrinityConfig cfg = TrinityConfig.new()\r\n    cfg.enabled := en\r\n    cfg.len_20 := l20\r\n    cfg.w_20 := w20\r\n    cfg.len_63 := l63\r\n    cfg.w_63 := w63\r\n    cfg.len_126 := l126\r\n    cfg.w_126 := w126\r\n    cfg.len_252 := l252\r\n    cfg.w_252 := w252\r\n    cfg.risk_floor := rf\r\n    cfg.risk_factor := fac\r\n    cfg.target_vol_stock := tvs\r\n    cfg.target_vol_safe := tvsf\r\n    cfg.gov_min_mult := gmm\r\n    cfg.k_len := kl\r\n    cfg.k_decay := kd\r\n    cfg.k_scale := ks\r\n    cfg.top_k := tk\r\n    cfg.baseline := bl\r\n    cfg.w_cp := wcp\r\n    cfg.w_cd := wcd\r\n    cfg.w_vs := wvs\r\n    cfg.w_ddg := wdd\r\n    cfg.shock_price := sp\r\n    cfg.shock_vol := sv\r\n    cfg.fall_thr := ft\r\n    cfg.rate_fall := rf2\r\n    cfg.rate_accel := ra\r\n    cfg.range_cp := r_cp\r\n    cfg.range_dd := r_dd\r\n    cfg.penalty_val := pen_v\r\n    cfg.ready_bars := rb\r\n    \r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5] = _core_compute(sym, cp, cs, s1, s2, s3, s4, s5, cfg)\r\n    [w, dl, df, sd1, sd2, sd3, sd4, sd5]\r\n\r\nexport core_build() => CORE_BUILD'
[CAPTURE 2] '//@version=5\r\n// @description TRINITY CORE LIBRARY v30.2 (Diamond Final / Export Fixed)\r\n// Architecture: Fixed 7 Calls / Raw Kernels / State Machine / Array Math\r\n// Constitution:\r\n// 1. Max 7 security calls.\r\n// 2. Bible Compliance: CD (Corr), DDG (Ratio), Dynamic Lengths.\r\n// 3. Ironclad Math: Volatility %, NaN-Guarded.\r\n// 4. Semantics: Correct Drop Codes, Safety Valve.\r\n// 5. API: calc_weights_flat returns FULL tuple [8] for Indicators.\r\nlibrary("TrinityCore30", overlay=false, dynamic_requests=true)\r\n\r\n// =============================================================================\r\n// 1. CONSTANTS & CONFIG\r\n// =============================================================================\r\nconst int READY_OK          = 0\r\nconst int NOT_ENOUGH_BARS   = 1\r\nconst int NA_INPUTS         = 2\r\nconst int SUM_FAIL          = 3\r\nconst int CASH_FAIL         = 4\r\n\r\nconst int DROP_OK           = 0\r\nconst int DROP_DISABLED     = 1\r\nconst int DROP_INVALID      = 2\r\nconst int DROP_BASELINE     = 3\r\nconst int DROP_NOT_TOPK     = 4\r\nconst int DROP_SHOCK        = 5\r\n\r\nconst float EPSILON = 1e-6\r\nconst int   CORE_BUILD = 300200\r\n\r\n// --- UDTs ---\r\nexport type TrinityWeights\r\n    float stock\r\n    float safe1\r\n    float safe2\r\n    float safe3\r\n    float safe4\r\n    float safe5\r\n    float cash\r\n\r\nexport type TrinityDiagLite\r\n    bool  is_ready\r\n    int   ready_code\r\n    int   core_build\r\n    bool  risk_gate\r\n    float wroc_stock\r\n    int   valid_safe_count\r\n    int   passed_safe_count\r\n    bool  sum_ok\r\n\r\nexport type TrinityDiagFull\r\n    float raw_stock\r\n    float mvol_stock\r\n    float mdecel_stock\r\n    float gov_stock\r\n    float safe_budget\r\n    int   top1_id\r\n    int   top2_id\r\n    float sum_weights\r\n\r\nexport type SafeDiag\r\n    bool  enabled\r\n    bool  valid\r\n    float sqs\r\n    float final_w\r\n    int   drop_code\r\n\r\nexport type TrinityConfig\r\n    bool  enabled = true\r\n    // Momentum\r\n    int   len_20  = 20\r\n    float w_20    = 12.0\r\n    int   len_63  = 63\r\n    float w_63    = 4.0\r\n    int   len_126 = 126\r\n    float w_126   = 2.0\r\n    int   len_252 = 252\r\n    float w_252   = 1.0\r\n    // Risk\r\n    float risk_floor  = 0.0\r\n    float risk_factor = 1.0\r\n    // Governor\r\n    float target_vol_stock = 15.0 // Percent\r\n    float target_vol_safe  = 8.0  // Percent\r\n    float gov_min_mult     = 0.1\r\n    // Decel\r\n    int   k_len   = 63\r\n    float k_decay = 0.99\r\n    float k_scale = 1.0 \r\n    // SQS\r\n    int   top_k = 2\r\n    float baseline = 0.5\r\n    float w_cp  = 0.3\r\n    float w_cd  = 0.2\r\n    float w_vs  = 0.2\r\n    float w_ddg = 0.3\r\n    // Penalties\r\n    float shock_price = 5.0\r\n    float shock_vol   = 50.0 // Percent\r\n    float fall_thr    = 3.0\r\n    float rate_fall   = 5.0\r\n    float rate_accel  = 2.0\r\n    float penalty_val = 0.5 \r\n    float range_cp    = 5.0  \r\n    float range_dd    = 10.0\r\n    // System\r\n    int   ready_bars = 252\r\n\r\n// Internal State\r\ntype SafeCalcState\r\n    int   id\r\n    float sqs\r\n    bool  passed\r\n    int   drop_code\r\n    float weight_share\r\n    float vol_gov\r\n\r\n// =============================================================================\r\n// 2. KERNELS (Dumb Data Providers)\r\n// =============================================================================\r\n\r\n// Unified Raw Kernel: Returns [Close, Open, Time]\r\n_k_raw() => [close, open, time]\r\n\r\n// =============================================================================\r\n// 3. HELPERS (Array Math for Stateless Core)\r\n// =============================================================================\r\n_sdiv(float n, float d) => (na(n) or na(d) or d == 0) ? 0.0 : n / d\r\n\r\n_gov_calc(float vol, float tgt, float min_m) => (na(vol) or vol <= 0) ? 1.0 : math.min(1.0, math.max(min_m, tgt / vol))\r\n\r\n// Score: Crash Protection (0..1)\r\n_score_cp(float roc, float range_val) =>\r\n    float den = 2.0 * range_val / 100.0\r\n    den == 0 ? 0.5 : (math.min(math.max(roc, -range_val/100.0), range_val/100.0) + range_val/100.0) / den\r\n\r\n// Score: Drawdown Gate (0..1)\r\n_score_ddg(float dd, float thr_val) =>\r\n    float den = thr_val / 100.0\r\n    den == 0 ? 0.0 : math.min(math.max(1.0 + (dd / den), 0.0), 1.0)\r\n\r\n// Helper: Get value from array with safety\r\n_get_val(float[] arr, int idx) =>\r\n    (array.size(arr) > idx) ? array.get(arr, idx) : na\r\n\r\n// Math: Volatility on Array of Log-Returns (Scaled to Annualized %)\r\n// Multiplier 1587.45 = sqrt(252) * 100\r\n_arr_vol(float[] logs, int len) =>\r\n    int sz = array.size(logs)\r\n    if sz < len\r\n        0.0\r\n    else\r\n        float sum = 0.0\r\n        float sum_sq = 0.0\r\n        for i = 0 to len - 1\r\n            float val = array.get(logs, i)\r\n            sum := sum + val\r\n            sum_sq := sum_sq + val * val\r\n        float mean = sum / len\r\n        float variance = (sum_sq / len) - (mean * mean)\r\n        math.sqrt(math.max(0.0, variance)) * 1587.45 \r\n\r\n// Math: K-Ratio on Array of Log-Prices\r\n_arr_k(float[] ln_p, int len) =>\r\n    int sz = array.size(ln_p)\r\n    if sz < len\r\n        0.0\r\n    else\r\n        float sum_x = 0.0\r\n        float sum_y = 0.0\r\n        float sum_xy = 0.0\r\n        float sum_x2 = 0.0\r\n        float sum_y2 = 0.0\r\n        float n = float(len)\r\n        for i = 0 to len - 1\r\n            float val = array.get(ln_p, i)\r\n            float x = float(len - 1 - i)\r\n            sum_x := sum_x + x\r\n            sum_y := sum_y + val\r\n            sum_xy := sum_xy + x * val\r\n            sum_x2 := sum_x2 + x * x\r\n            sum_y2 := sum_y2 + val * val\r\n        \r\n        float slope_num = n * sum_xy - sum_x * sum_y\r\n        float slope_den = n * sum_x2 - sum_x * sum_x\r\n        float slope = slope_den != 0 ? slope_num / slope_den : 0.0\r\n        \r\n        float intercept = (sum_y - slope * sum_x) / n\r\n        float sum_err_sq = 0.0\r\n        for i = 0 to len - 1\r\n            float val = array.get(ln_p, i)\r\n            float x = float(len - 1 - i)\r\n            float pred = slope * x + intercept\r\n            float err = val - pred\r\n            sum_err_sq := sum_err_sq + err * err\r\n        \r\n        float rmse = math.sqrt(sum_err_sq / n)\r\n        float k = (rmse > 0 and slope != 0) ? (slope / rmse) * math.sqrt(n) : 0.0\r\n        k * 15.87 // Annualize\r\n\r\n// Math: Correlation on two arrays of changes (length 20)\r\n_arr_corr(float[] arr1, float[] arr2, int len) =>\r\n    int sz1 = array.size(arr1)\r\n    int sz2 = array.size(arr2)\r\n    int lim = math.min(len, math.min(sz1, sz2))\r\n    if lim < 2\r\n        0.0\r\n    else\r\n        float sum_x = 0.0\r\n        float sum_y = 0.0\r\n        float sum_xy = 0.0\r\n        float sum_x2 = 0.0\r\n        float sum_y2 = 0.0\r\n        for i = 0 to lim - 1\r\n            float x = array.get(arr1, i)\r\n            float y = array.get(arr2, i)\r\n            sum_x := sum_x + x\r\n            sum_y := sum_y + y\r\n            sum_xy := sum_xy + x * y\r\n            sum_x2 := sum_x2 + x * x\r\n            sum_y2 := sum_y2 + y * y\r\n        float n = float(lim)\r\n        float num = n * sum_xy - sum_x * sum_y\r\n        float den = math.sqrt((n * sum_x2 - sum_x * sum_x) * (n * sum_y2 - sum_y * sum_y))\r\n        den == 0 ? 0.0 : num / den\r\n\r\n// =============================================================================\r\n// 4. CORE LOGIC\r\n// =============================================================================\r\n_core_compute(string sym_stk, string cp, string cs, string s1, string s2, string s3, string s4, string s5, TrinityConfig cfg) =>\r\n    \r\n    // --- 1. DATA FETCHING (7 Calls, Raw) ---\r\n    string stk_sym = str.length(sym_stk) > 0 ? sym_stk : syminfo.tickerid\r\n    [stk_c, stk_o, stk_t] = request.security(stk_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string csh_raw = str.length(cp) > 0 ? cp : cs\r\n    string csh_sym = str.length(csh_raw) > 0 ? csh_raw : syminfo.tickerid\r\n    [csh_c, csh_o, csh_t] = request.security(csh_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string s1_sym = str.length(s1)>0 ? s1 : syminfo.tickerid\r\n    [sf1_c, sf1_o, sf1_t] = request.security(s1_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string s2_sym = str.length(s2)>0 ? s2 : syminfo.tickerid\r\n    [sf2_c, sf2_o, sf2_t] = request.security(s2_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string s3_sym = str.length(s3)>0 ? s3 : syminfo.tickerid\r\n    [sf3_c, sf3_o, sf3_t] = request.security(s3_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string s4_sym = str.length(s4)>0 ? s4 : syminfo.tickerid\r\n    [sf4_c, sf4_o, sf4_t] = request.security(s4_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    string s5_sym = str.length(s5)>0 ? s5 : syminfo.tickerid\r\n    [sf5_c, sf5_o, sf5_t] = request.security(s5_sym, "D", _k_raw(), lookahead=barmerge.lookahead_off, ignore_invalid_symbol=true)\r\n    \r\n    // --- 2. STATE MACHINE (Daily Updates) ---\r\n    var int last_d_t = 0\r\n    bool new_day = (stk_t != last_d_t) and (not na(stk_t))\r\n    \r\n    // Cash State\r\n    var float csh_synth = 100.0\r\n    var float csh_ref = na\r\n    var float csh_val = 0.0\r\n    var bool cash_inited = false\r\n    \r\n    // History Arrays\r\n    int hist_len = 300 \r\n    var float[] h_stk_c = array.new_float(0)\r\n    var float[] h_csh_c = array.new_float(0)\r\n    var float[] h_sf1_c = array.new_float(0)\r\n    var float[] h_sf2_c = array.new_float(0)\r\n    var float[] h_sf3_c = array.new_float(0)\r\n    var float[] h_sf4_c = array.new_float(0)\r\n    var float[] h_sf5_c = array.new_float(0)\r\n    \r\n    var float[] h_stk_ln = array.new_float(0) \r\n    var float[] h_stk_lret = array.new_float(0) \r\n    \r\n    var float[] h_r_stk = array.new_float(0)\r\n    var float[] h_r_sf1 = array.new_float(0)\r\n    var float[] h_r_sf2 = array.new_float(0)\r\n    var float[] h_r_sf3 = array.new_float(0)\r\n    var float[] h_r_sf4 = array.new_float(0)\r\n    var float[] h_r_sf5 = array.new_float(0)\r\n    \r\n    var float[] h_r_stk_chg = array.new_float(0)\r\n    var float[] h_r_sf1_chg = array.new_float(0)\r\n    var float[] h_r_sf2_chg = array.new_float(0)\r\n    var float[] h_r_sf3_chg = array.new_float(0)\r\n    var float[] h_r_sf4_chg = array.new_float(0)\r\n    var float[] h_r_sf5_chg = array.new_float(0)\r\n    \r\n    var float stk_k_peak = 0.0\r\n    \r\n    if new_day\r\n        last_d_t := stk_t\r\n        cash_inited := true\r\n        \r\n        // Cash Logic\r\n        float r = 16.0 \r\n        int y = year(stk_t)\r\n        if y <= 2014\r\n            r := 9.0\r\n        else if y == 2015\r\n            r := 12.0\r\n        else if y >= 2016 and y <= 2019\r\n            r := 7.5\r\n        else if y == 2020\r\n            r := 5.0\r\n        else if y == 2021\r\n            r := 7.0\r\n        else if y == 2022\r\n            r := 14.0\r\n        else if y == 2023\r\n            r := 12.0\r\n        csh_synth := csh_synth * (1.0 + (r / 100.0) / 365.0)\r\n        \r\n        if na(csh_ref) and not na(csh_c) and csh_c != 0\r\n            csh_ref := csh_synth / csh_c\r\n        \r\n        csh_val := (not na(csh_c) and csh_c != 0) ? csh_c * nz(csh_ref, 1.0) : csh_synth\r\n        \r\n        // Push History\r\n        array.unshift(h_stk_c, stk_c)\r\n        array.unshift(h_csh_c, csh_val)\r\n        array.unshift(h_sf1_c, sf1_c)\r\n        array.unshift(h_sf2_c, sf2_c)\r\n        array.unshift(h_sf3_c, sf3_c)\r\n        array.unshift(h_sf4_c, sf4_c)\r\n        array.unshift(h_sf5_c, sf5_c)\r\n        \r\n        array.unshift(h_stk_ln, stk_c > 0 ? math.log(stk_c) : 0.0)\r\n        \r\n        float prev_stk = array.size(h_stk_c) > 1 ? array.get(h_stk_c, 1) : na\r\n        array.unshift(h_stk_lret, (stk_c > 0 and prev_stk > 0) ? math.log(stk_c/prev_stk) : 0.0)\r\n        \r\n        float r_stk = _sdiv(stk_c, csh_val)\r\n        float r_sf1 = _sdiv(sf1_c, csh_val)\r\n        float r_sf2 = _sdiv(sf2_c, csh_val)\r\n        float r_sf3 = _sdiv(sf3_c, csh_val)\r\n        float r_sf4 = _sdiv(sf4_c, csh_val)\r\n        float r_sf5 = _sdiv(sf5_c, csh_val)\r\n        \r\n        array.unshift(h_r_stk, r_stk)\r\n        array.unshift(h_r_sf1, r_sf1)\r\n        array.unshift(h_r_sf2, r_sf2)\r\n        array.unshift(h_r_sf3, r_sf3)\r\n        array.unshift(h_r_sf4, r_sf4)\r\n        array.unshift(h_r_sf5, r_sf5)\r\n        \r\n        float prev_r_stk = array.size(h_r_stk) > 1 ? array.get(h_r_stk, 1) : r_stk\r\n        float prev_r_sf1 = array.size(h_r_sf1) > 1 ? array.get(h_r_sf1, 1) : r_sf1\r\n        float prev_r_sf2 = array.size(h_r_sf2) > 1 ? array.get(h_r_sf2, 1) : r_sf2\r\n        float prev_r_sf3 = array.size(h_r_sf3) > 1 ? array.get(h_r_sf3, 1) : r_sf3\r\n        float prev_r_sf4 = array.size(h_r_sf4) > 1 ? array.get(h_r_sf4, 1) : r_sf4\r\n        float prev_r_sf5 = array.size(h_r_sf5) > 1 ? array.get(h_r_sf5, 1) : r_sf5\r\n        \r\n        array.unshift(h_r_stk_chg, r_stk - prev_r_stk)\r\n        array.unshift(h_r_sf1_chg, r_sf1 - prev_r_sf1)\r\n        array.unshift(h_r_sf2_chg, r_sf2 - prev_r_sf2)\r\n        array.unshift(h_r_sf3_chg, r_sf3 - prev_r_sf3)\r\n        array.unshift(h_r_sf4_chg, r_sf4 - prev_r_sf4)\r\n        array.unshift(h_r_sf5_chg, r_sf5 - prev_r_sf5)\r\n        \r\n        if array.size(h_stk_c) > hist_len\r\n            array.pop(h_stk_c)\r\n            array.pop(h_csh_c)\r\n            array.pop(h_sf1_c)\r\n            array.pop(h_sf2_c)\r\n            array.pop(h_sf3_c)\r\n            array.pop(h_sf4_c)\r\n            array.pop(h_sf5_c)\r\n            array.pop(h_stk_ln)\r\n            array.pop(h_stk_lret)\r\n            array.pop(h_r_stk)\r\n            array.pop(h_r_sf1)\r\n            array.pop(h_r_sf2)\r\n            array.pop(h_r_sf3)\r\n            array.pop(h_r_sf4)\r\n            array.pop(h_r_sf5)\r\n            array.pop(h_r_stk_chg)\r\n            array.pop(h_r_sf1_chg)\r\n            array.pop(h_r_sf2_chg)\r\n            array.pop(h_r_sf3_chg)\r\n            array.pop(h_r_sf4_chg)\r\n            array.pop(h_r_sf5_chg)\r\n            \r\n    // --- 3. METRIC CALCULATION (Declarations for Hoisting) ---\r\n    TrinityWeights w = TrinityWeights.new(0,0,0,0,0,0,1)\r\n    TrinityDiagFull df = TrinityDiagFull.new(0,0,0,0,0,0,0,0)\r\n    \r\n    SafeDiag sd_null = SafeDiag.new(false, false, 0, 0, DROP_INVALID)\r\n    SafeDiag sd1 = sd_null\r\n    SafeDiag sd2 = sd_null\r\n    SafeDiag sd3 = sd_null\r\n    SafeDiag sd4 = sd_null\r\n    SafeDiag sd5 = sd_null\r\n    \r\n    // Hoisted Variables\r\n    float final_stock = 0.0\r\n    float final_cash = 1.0 \r\n    float w1=0.0, w2=0.0, w3=0.0, w4=0.0, w5=0.0\r\n    float raw_stk=0.0, mvol=0.0, mdec=0.0, gov_final=0.0, safe_budget=0.0\r\n    float used_safe_alloc = 0.0\r\n    int t1_id = 0\r\n    int t2_id = 0\r\n    \r\n    int valid_cnt = 0\r\n    int valid_pass = 0\r\n    bool sum_check = false\r\n    float wroc = 0.0\r\n    bool risk_gate = false\r\n    \r\n    // Strict Readiness Checks\r\n    bool inputs_ok = not na(stk_c) and stk_c > 0\r\n    float c0 = _get_val(h_csh_c, 0)\r\n    bool cash_bad = cash_inited and (na(c0) or c0 <= 0)\r\n    \r\n    int max_mom = math.max(cfg.len_20, math.max(cfg.len_63, math.max(cfg.len_126, cfg.len_252)))\r\n    int max_arch = 252 \r\n    int req_hist = math.max(cfg.ready_bars, math.max(max_mom, math.max(cfg.k_len, max_arch)))\r\n    bool hist_ok = array.size(h_stk_c) >= req_hist\r\n    \r\n    // Window Validity\r\n    float w_check1 = _get_val(h_r_stk, 0)\r\n    float w_check2 = _get_val(h_r_stk, max_mom)\r\n    bool window_ok = not na(w_check1) and not na(w_check2)\r\n    \r\n    int rc = READY_OK\r\n    if not inputs_ok\r\n        rc := NA_INPUTS\r\n    else if cash_bad\r\n        rc := CASH_FAIL\r\n    else if not hist_ok or not window_ok\r\n        rc := NOT_ENOUGH_BARS\r\n        \r\n    bool is_ready = (rc == READY_OK) and cfg.enabled\r\n    \r\n    if is_ready\r\n        // WROC\r\n        float r_stk_now = _get_val(h_r_stk, 0)\r\n        float roc20  = _sdiv(r_stk_now, _get_val(h_r_stk, cfg.len_20)) - 1.0\r\n        float roc63  = _sdiv(r_stk_now, _get_val(h_r_stk, cfg.len_63)) - 1.0\r\n        float roc126 = _sdiv(r_stk_now, _get_val(h_r_stk, cfg.len_126)) - 1.0\r\n        float roc252 = _sdiv(r_stk_now, _get_val(h_r_stk, cfg.len_252)) - 1.0\r\n        float w_sum = cfg.w_20 + cfg.w_63 + cfg.w_126 + cfg.w_252\r\n        wroc := (roc20*cfg.w_20 + roc63*cfg.w_63 + roc126*cfg.w_126 + roc252*cfg.w_252) / (w_sum == 0 ? 1.0 : w_sum)\r\n\r\n        // Stock Gov\r\n        float stk_vol = _arr_vol(h_stk_lret, 20)\r\n        float stk_k   = _arr_k(h_stk_ln, math.max(2, cfg.k_len))\r\n        \r\n        // Decay logic inside core\r\n        if new_day\r\n            stk_k_peak := stk_k_peak * cfg.k_decay\r\n            if stk_k > stk_k_peak\r\n                stk_k_peak := stk_k\r\n                \r\n        mvol := _gov_calc(stk_vol, cfg.target_vol_stock, cfg.gov_min_mult)\r\n        mdec := 1.0\r\n        if stk_k_peak > 0\r\n            float ratio = stk_k / stk_k_peak\r\n            float val_clamped = math.max(ratio, 0.0)\r\n            float ks = cfg.k_scale == 0 ? 1.0 : cfg.k_scale\r\n            float val_scaled = cfg.k_scale != 1.0 ? val_clamped / ks : val_clamped\r\n            mdec := math.min(1.0, val_scaled)\r\n        \r\n        gov_final := mvol * mdec\r\n        \r\n        float rf_safe = cfg.risk_factor == 0 ? 1.0 : cfg.risk_factor\r\n        if cfg.risk_floor > 0\r\n            raw_stk := math.max(cfg.